{
  "name": "@mecenate/hardhat",
  "version": "0.0.1",
  "scripts": {
    "account": "hardhat run scripts/listAccount.ts",
    "chain": "hardhat node --network hardhat --no-deploy",
    "compile": "hardhat compile",
    "deploy": "hardhat deploy --export-all ../nextjs/generated/hardhat_contracts.json",
    "fork": "MAINNET_FORKING_ENABLED=true hardhat node --network hardhat --no-deploy",
    "generate": "hardhat run scripts/generateAccount.ts",
    "lint": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore ./*.ts ./deploy/**/*.ts ./scripts/**/*.ts ./tests/**/*.ts",
    "lint-staged": "eslint --config ./.eslintrc.json --ignore-path ./.eslintignore",
    "test": "REPORT_GAS=true hardhat test --network hardhat",
    "verify": "hardhat etherscan-verify",
    "coverage": "hardhat coverage --network hardhat",
    "prettier": "prettier --write 'contracts/**/*.sol'"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-solhint": "^2.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.3",
    "@types/eslint": "^8",
    "@types/mocha": "^9.1.1",
    "@types/prettier": "^2",
    "@types/qrcode": "^1",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chai": "^4.3.6",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethers": "^5.7.1",
    "hardhat": "2.13.0",
    "hardhat-deploy": "^0.11.15",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "2.7.1",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "3box": "^1.19.1",
    "@chainlink/contracts": "^0.6.1",
    "@erasure/crypto-ipfs": "^1.3.2",
    "@libp2p/crypto": "^1.0.14",
    "@openzeppelin/contracts": "^4.8.2",
    "@pinata/sdk": "^2.1.0",
    "@polkadot/util": "^11.0.2",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "bs58": "^4.0.1",
    "crypto": "^1.0.1",
    "crypto-js": "^3.1.8",
    "dotenv": "^16.0.3",
    "envfile": "^6.18.0",
    "eth-crypto": "^2.6.0",
    "ethereum-waffle": "^4.0.10",
    "ipfs": "^0.61.0",
    "ipfs-mini": "^1.1.5",
    "it-all": "^1.0.6",
    "libp2p": "^0.35.8",
    "libp2p-crypto": "^0.21.2",
    "node-gyp": "^9.3.1",
    "qrcode": "^1.5.1",
    "secp256k1": "^5.0.0",
    "tweetnacl": "^1.0.3"
  },
  "resolution": {
    "blockstore-core": "1.0.2"
  }
}
